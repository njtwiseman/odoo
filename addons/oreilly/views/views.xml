<odoo>
  <data>
	<!-- explicit list view definition -->
	<record model="ir.ui.view" id="oreilly.list">
	  <field name="name">oreilly list</field>
	  <field name="model">oreilly.oreilly</field>
	  <field name="arch" type="xml">
		<tree>
		  <field name="name"/>
		  <field name="value"/>
		  <field name="value2"/>
		</tree>
	  </field>
	</record>
	<record model="ir.actions.server" id="oreilly.order_action_server">
	  <field name="name">oreilly order server</field>
	  <field name="model_id" ref="model_oreilly_order"/>
	</record>

	<record model="ir.actions.act_window" id="oreilly.vehicle_action_window">
	  <field name="name">oreilly vehicle window</field>
	  <field name="res_model">oreilly.vehicle</field>
	  <field name="view_mode">tree,form</field>
	</record>

	<record model="ir.actions.act_window" id="oreilly.order_action_window">
	  <field name="name">oreilly order window</field>
	  <field name="res_model">oreilly.order</field>
	  <field name="view_mode">tree,form</field>
	</record>

	<record model="ir.actions.act_window" id="oreilly.update_action_window">
	  <field name="name">oreilly update window</field>
	  <field name="res_model">oreilly.update</field>
	  <field name="view_mode">tree,form</field>
	</record>
	<!--
	<record model="ir.actions.server" id="oreilly.update_action">
	  <field name="name">Oreilly Updater</field>
	  <field name="model_id" ref="model_oreilly_update"/>
	  <field name="code">
		  1 
	  </field>
	</record>
	-->






	<!-- Top menu item -->
	<menuitem name="oreilly" id="oreilly.menu_root"/>

	  <!-- menu categories -->
	<menuitem name="Orders" id="oreilly.orders" parent="oreilly.menu_root"
	  action="oreilly.order_action_window"/>
	<menuitem name="Vehicles" id="oreilly.vehicles" parent="oreilly.menu_root"
	  action="oreilly.vehicle_action_window"/>
	<menuitem name="Update" id="oreilly.update" parent="oreilly.menu_root"
	  action="oreilly.update_action_window"/>
  </data>
</odoo>
