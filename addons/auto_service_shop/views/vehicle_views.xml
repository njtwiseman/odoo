<odoo>
  <data>
    <record model="ir.ui.view" id="vehicle_vehicle_list">
      <field name="name">vehicle list</field>
      <field name="model">vehicle.vehicle</field>
      <field name="arch" type="xml">
        <tree>
          <field name="vin"/>
          <field name="licensePlate"/>
          <field name="ymm"/>
        </tree>
      </field>
    </record>

    <menuitem name="Vehicles" id="vehicle_menu_root"/>
    <menuitem name="Year" id="vehicle_year" parent="vehicle_menu_root"/>
    <menuitem name="Make" id="vehicle_make" parent="vehicle_menu_root"/>
    <menuitem name="Model" id="vehicle_model" parent="vehicle_menu_root"/>

  </data>
</odoo>
