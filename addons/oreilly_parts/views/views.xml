<odoo>
  <data>
    <record model="ir.ui.view" id="oreilly_parts.list">
      <field name="name">oreilly_parts list</field>
      <field name="model">oreilly_parts.order</field>
      <field name="priority" eval="1"/>
      <field name="arch" type="xml">
        <tree>
		<field name="ident"/>
		<field name="shopCost"/>
	</tree>
		<form string="Idea form">
	  	  <group colspan="4">
	        	<group colspan="2" col="2">
	        	    <separator string="General stuff" colspan="2"/>
	        	    <field name="ident"/>
	        	</group>
	
	        	<group colspan="2" col="2">
	        	    <separator string="Dates" colspan="2"/>
	        	    <field name="worksheetState"/>
			    <field name="shopCost" readonly="1"/>
			    <field name="totalPrice"/>
	        	</group>
	
	        	<notebook colspan="4">
	        	    <page string="Description">
	        	        <field name="descFragment" nolabel="1"/>
	        	    </page>
	        	</notebook>
	
	    	</group>
	</form>
    </field>
  </record>
 
  <record model="ir.actions.act_window" id="oreilly_parts.action_window">
      <field name="name">oreilly_parts window</field>
      <field name="res_model">oreilly_parts.order</field>
      <field name="view_type">tree</field>
      <field name="view_mode">tree,form</field>
  </record>
  <menuitem name="main_oreilly_parts_menu" id="oreilly_parts.menu_root"/>
  <menuitem name="Menu 1" id="oreilly_parts.menu_1" parent="oreilly_parts.menu_root"/>
  <menuitem name="Menu 2" id="oreilly_parts.menu_2" parent="oreilly_parts.menu_root"/>
  <menuitem name="List" id="oreilly_parts.menu_1_list" parent="oreilly_parts.menu_1" action="oreilly_parts.action_window"/>
</data>
</odoo>
